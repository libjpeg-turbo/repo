[libjpeg-turbo]
name=libjpeg-turbo official RPMs
baseurl=https://packagecloud.io/dcommander/libjpeg-turbo/rpm_any/rpm_any/$basearch
repo_gpgcheck=1
gpgcheck=1
gpgkey=https://packagecloud.io/dcommander/libjpeg-turbo/gpgkey
       https://raw.githubusercontent.com/libjpeg-turbo/repo/main/LJT-GPG-KEY
       https://raw.githubusercontent.com/libjpeg-turbo/repo/main/LJT-GPG-KEY-1024
enabled=1
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300
exclude=libjpeg-turbo-[0-9]*

[libjpeg-turbo-source]
name=libjpeg-turbo official SRPMs
baseurl=https://packagecloud.io/dcommander/libjpeg-turbo/rpm_any/rpm_any/SRPMS
repo_gpgcheck=1
gpgcheck=1
gpgkey=https://packagecloud.io/dcommander/libjpeg-turbo/gpgkey
       https://raw.githubusercontent.com/libjpeg-turbo/repo/main/LJT-GPG-KEY
       https://raw.githubusercontent.com/libjpeg-turbo/repo/main/LJT-GPG-KEY-1024
enabled=1
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300
exclude=libjpeg-turbo-[0-9]*
