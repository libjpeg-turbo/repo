[libjpeg-turbo-i386]
name=libjpeg-turbo official i386 RPMs
baseurl=https://packagecloud.io/dcommander/libjpeg-turbo/rpm_any/rpm_any/i386
repo_gpgcheck=1
gpgcheck=1
gpgkey=https://packagecloud.io/dcommander/libjpeg-turbo/gpgkey
       https://raw.githubusercontent.com/libjpeg-turbo/repo/main/LJT-GPG-KEY
       https://raw.githubusercontent.com/libjpeg-turbo/repo/main/LJT-GPG-KEY-1024
enabled=1
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
exclude=libjpeg-turbo-[0-9]*
